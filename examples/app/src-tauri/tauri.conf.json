{
    "productName": "tauri-specta-example",
    "version": "0.0.0",
    "build": {
        "beforeDevCommand": "pnpm vite-dev",
        "beforeBuildCommand": "pnpm vite-build",
        "devUrl": "http://localhost:1420",
        "frontendDist": "../dist"
    },
    "identifier": "com.otbeaumont.tauri-specta-example",
    "bundle": {
        "active": true,
        "category": "DeveloperTool",
        "copyright": "",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "targets": "all"
    },
    "app": {
        "security": {
            "csp": null,
            "assetProtocol": {
                "enable": true
            }
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 600,
                "resizable": true,
                "title": "tauri-specta-example",
                "width": 800,
                "fileDropEnabled": false,
                "label": "main",
                "minHeight": 600,
                "minWidth": 800,
                "visible": true
            }
        ],
        "withGlobalTauri": true
    }
}
